<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"        "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.market.pos.mapper.HongMapper">    <resultMap id="HongResultMap" type="com.market.pos.pojo.Hong">        <id property="id" column="id"/>        <result property="userid" column="userid"/>        <result property="type" column="type"/>        <result property="content" column="content"/>    </resultMap>    <!--通过userid查询-->    <select id="getHongByUserIdAndType" resultType="String">        select content from hong where userid = #{userid} and type = #{type}    </select>    <!--数据更新-->    <update id="updateHongByUserId" parameterType="Hong">        update hong        <trim prefix="set" suffixOverrides=",">            <if test="content != null">                content = #{content}            </if>        </trim>        where userid = #{userid} and type = #{type}    </update>    <!--数据插入-->    <insert id="insertHong">        insert into hong (userid, type, content) values (#{userid},#{type},#{content});    </insert></mapper>