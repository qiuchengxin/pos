<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"        "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.market.pos.mapper.ITeamMembersMapper">    <resultMap id="TeamMembersResultMap" type="com.market.pos.pojo.TeamMembers">        <id property="tId" column="t_id"/>        <result property="userid" column="userid"/>        <result property="username" column="username"/>        <result property="usertype" column="usertype"/>        <result property="isTibu" column="is_tibu"/>    </resultMap>    <!--新增用户-->    <insert id="addTeamMember" parameterType="TeamMembers" keyProperty="tId" useGeneratedKeys="true">        insert into team_members (t_id, userid, username, usertype, is_tibu) values                                (#{tId},#{userid},#{username},#{usertype},#{isTibu});    </insert>    <select id="findById" resultMap="TeamMembersResultMap">        select * from team_members where t_id = #{tId}    </select></mapper>