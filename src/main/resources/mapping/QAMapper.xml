<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"        "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.market.pos.mapper.QAMapper">    <resultMap id="QAResultMap" type="com.market.pos.pojo.Qa">        <id property="id" column="id"/>        <result property="question" column="question"/>        <result property="answer" column="answer"/>        <result property="userid" column="userid"/>    </resultMap>    <!--admin查询list-->    <select id="getAllQA" resultMap="QAResultMap">        select * from Qa;    </select>    <!--通过userid查询-->    <select id="getQAByUserId" resultMap="QAResultMap">        select * from qa where userid = #{userid};    </select>    <!--查询问题-->    <select id="getQuestion" resultType="String">        select question from qa where question = #{quetion};    </select>    <!--数据更新-->    <update id="updateQAById" parameterType="Qa">        update qa        <trim prefix="set" suffixOverrides=",">            <if test="answer != null">                answer = #{answer}            </if>        </trim>        where id = #{id};    </update>    <!--数据插入-->    <insert id="insertQA">        insert into Qa (question, answer,userid) values (#{question},#{answer},#{userid});    </insert>    <delete id="delQA">        delete from qa where id = #{id};    </delete></mapper>